{
  "name": "gemini-cli-button-vscode",
  "displayName": "Gemini CLI Button For Visual Studio Code",
  "description": "Adds a button to the editor title bar to open Gemini CLI in VS Code.",
  "version": "0.0.1",
  "license": "MIT",
  "publisher": "Axtra",
  "keywords": [
    "gemini",
    "ai",
    "cli",
    "terminal",
    "button"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Axtraa/gemini-cli-button-vscode.git"
  },
  "icon": "resources/gemini-app-icon.png",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gemini-button.run",
        "title": "Run Gemini",
        "icon": "resources/gemini.png"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "gemini-button.run",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2"
  }
}
